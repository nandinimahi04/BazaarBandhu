{
    "framework": "vite",
    "buildCommand": "npm install --include=dev && npm run build:client",
    "outputDirectory": "dist/spa",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "https://bazaarbandhu.onrender.com/api/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}